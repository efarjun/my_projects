aws cloudwatch put-metric-alarm --alarm-name "CPUUtilization-<name>" --alarm-description CS0096306 --alarm-actions arn:aws:sns:us-east-1:<account number>:alerts --metric-name CPUUtilization --namespace AWS/EC2 --threshold 60 --period 300 --statistic Average --comparison-operator GreaterThanThreshold --dimensions "Name=InstanceId,Value=<instance-id>" --unit Percent --evaluation-periods 1 --region us-east-1 --profile <profile name>
